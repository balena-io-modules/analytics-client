{
  "name": "analytics-client",
  "version": "2.0.2",
  "description": "Convenient builders to compose analytics tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/balena-io/analytics-client"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "prettify": "balena-lint --typescript --fix src/ test/ index.ts",
    "lint": "balena-lint -src test index.ts",
    "build": "npm run lint && npm run test && tsc && webpack",
    "prepublish": "npm run build"
  },
  "files": [
    "dist/",
    "src/",
    "index.ts"
  ],
  "dependencies": {
    "@amplitude/analytics-browser": "^1.5.4",
    "@amplitude/marketing-analytics-browser": "^0.2.4",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@balena/lint": "^9.3.0",
    "@types/jest": "^27.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.2",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.9.5",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --typescript --fix"
    ],
    "test/**/*.ts": [
      "balena-lint --typescript --no-prettier --tests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "versionist": {
    "publishedAt": "2023-07-14T18:37:52.760Z"
  }
}
