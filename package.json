    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/mixpanel-browser": "^2.38.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.2",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "*.ts": [
